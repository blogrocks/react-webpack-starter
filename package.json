  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "visualize": "npm run build && open dist/stats.html",
    "build-vendor": "npm run clean && cross-env NODE_ENV=production webpack --config vendor-bundles.webpack.prod.js -p --optimize-minimize",
    "build": "npm run build-vendor && cross-env NODE_ENV=production webpack --config webpack.prod.js -p --optimize-minimize",
    "dev-vendor": "webpack --config vendor-bundles.webpack.dev.js",
    "start": "npm run dev-vendor && webpack-dev-server --open --config webpack.dev.js",
    "bundle-out": "npm run dev-vendor && webpack --config webpack.dev.js"
  },
